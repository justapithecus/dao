{
  "name": "Dao Workspace",
  "dockerComposeFile": [
    "docker-compose.dev.yaml"
  ],
  "service": "workspace",
  "workspaceFolder": "/workspace/dao",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "editorconfig.editorconfig",
        "llvm-vs-code-extensions.vscode-clangd",
        "revng.llvm-ir"
      ]
    }
  }
}
